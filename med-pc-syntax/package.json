{
    "name": "med-pc-syntax",
    "displayName": "Med-PC Syntax",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mpc",
            "aliases": ["MedPC", "mpc"],
            "extensions": [".mpc",".pmpc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mpc",
            "scopeName": "source.mpc",
            "path": "./syntaxes/mpc.tmLanguage.json"
        }]
    }
}